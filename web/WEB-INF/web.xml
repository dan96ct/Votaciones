<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>Controlador_altaVotante</servlet-name>
        <servlet-class>Controlador.Controlador_altaVotante</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Controlador_votar</servlet-name>
        <servlet-class>Controlador.Controlador_votar</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Controlador_censo</servlet-name>
        <servlet-class>Controlador.Controlador_censo</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Controlador_mostrarPartidos</servlet-name>
        <servlet-class>Controlador.Controlador_mostrarPartidos</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Controlador_bajaVotante</servlet-name>
        <servlet-class>Controlador.Controlador_bajaVotante</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Controlador_altaVotante</servlet-name>
        <url-pattern>/Controlador_altaVotante</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Controlador_votar</servlet-name>
        <url-pattern>/Controlador_votar</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Controlador_censo</servlet-name>
        <url-pattern>/Controlador_censo</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Controlador_mostrarPartidos</servlet-name>
        <url-pattern>/Controlador_mostrarPartidos</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Controlador_bajaVotante</servlet-name>
        <url-pattern>/Controlador_bajaVotante</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
